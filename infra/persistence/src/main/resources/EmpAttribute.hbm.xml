<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="EmpAttribute" table="roles">
    <meta attribute="class-description">
      This class contains the roles for each employee.
    </meta>
    <property name="empId" column="emp_id" type="int"/>
    <property name="attrId" column="attr_id" type="int"/>
    <property name="status" column="contact_status" type="boolean"/>
  </class>
</hibernate-mapping>

