<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ecc.ems">
  <class name="Contact" table="contact">
    <meta attribute="class-description">
      This class contains the contact type detail.
    </meta>
    <id name="id" type="int" column="contact_id">
      <generator class="sequence"/>
    </id>
    <property name="contactType" column="contact_type" type="string"/>
    <property name="contactDetails" column="contact_details" type="string"/>
    <property name="status" column="contact_status" type="boolean"/>
  </class>
</hibernate-mapping>

